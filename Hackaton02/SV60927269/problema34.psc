Proceso prob34
    Definir i, j Como Entero;
	
    // Iterar a través de los números del 1 al 9
    Para i <- 1 Hasta 9 Hacer
        Escribir "Tabla de multiplicar del ", i, ":";
        
        // Iterar para cada número de la tabla de multiplicar
        Para j <- 1 Hasta 10 Hacer
            Escribir i, " * ", j, " = ", i * j;
        FinPara
		
        // Espacio en blanco para separar las tablas
        Escribir "";
    FinPara
FinProceso
