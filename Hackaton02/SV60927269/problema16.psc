Proceso prob16
	
    Definir num Como Entero;
	
    
    Escribir "Ingrese un número del 1 al 7:";
    Leer num;
	
   
    Segun numero Hacer
        1: Escribir "Lunes";
        2: Escribir "Martes";
        3: Escribir "Miércoles";
        4: Escribir "Jueves";
        5: Escribir "Viernes";
        6: Escribir "Sábado";
        7: Escribir "Domingo";
		De Otro Modo:
			Escribir "El número no está en el rango permitido.";
    FinSegun;
FinProceso
